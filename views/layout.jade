doctype html
html
  head
    title Corona
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel='stylesheet', href='/css/foundation.css')
    link(rel='stylesheet', href='/css/custom.css')
    script(src='//use.typekit.net/jge0btp.js')
    script try{Typekit.load();}catch(e){}
    style [ng-cloak] { display: none; }

  body(ng-app='corona')

    block content
  
  script(src='/js/vendor/jquery.js')
  script(src='/js/foundation.min.js')
  script(src='/js/foundation/foundation.dropdown.js')
  script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.2.16/angular.min.js')
  script(src='/js/corona.js')
  script(src='/js/directives/todoEscape.js')
  script(src='/js/directives/todoFocus.js')
  script.
    $(document).foundation();
  script.
    window.onload = function() {
        if (window.jQuery) {  
            // jQuery is loaded  
            alert("Yeah!");
        } else {
            // jQuery is not loaded
            alert("Doesn't Work");
        }
    }